<p id="notice"><%= notice %></p>

<h1>Setting Apis</h1>

<table>
  <thead>
    <tr>
      <th>Email access</th>
      <th>Token access</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @setting_apis.each do |setting_api| %>
      <tr>
        <td><%= setting_api.email_access %></td>
        <td><%= setting_api.token_access %></td>
        <td><%= link_to 'Show', setting_api %></td>
        <td><%= link_to 'Edit', edit_setting_api_path(setting_api) %></td>
        <td><%= link_to 'Destroy', setting_api, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Setting Api', new_setting_api_path %>
